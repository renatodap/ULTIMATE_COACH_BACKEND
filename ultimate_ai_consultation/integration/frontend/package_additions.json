{
  "_comment": "Add these dependencies to ULTIMATE_COACH_FRONTEND/package.json",
  "dependencies": {
    "axios": "^1.6.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^13.14.0"
  },
  "installation": {
    "npm": "npm install axios react-native-chart-kit react-native-svg",
    "yarn": "yarn add axios react-native-chart-kit react-native-svg"
  },
  "notes": [
    "axios: HTTP client for API requests",
    "react-native-chart-kit: Chart library for weight trends and progress",
    "react-native-svg: Required dependency for react-native-chart-kit",
    "For iOS: Run 'npx pod-install' after installation"
  ]
}
